<!DOCTYPE html>
<html lang="{{ locale }}" dir="{{ direction }}" class="{{ checkout_html_classes }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, height=device-height, minimum-scale=1.0, user-scalable=0">
    <meta name="referrer" content="origin">
    <link rel="preconnect" href="https://fonts.shopifycdn.com">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css">
    <title>{{ page_title }}</title>

    {{ content_for_header }}

    {{ checkout_stylesheets }}
    {{ checkout_scripts }}
    <script
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBLjNHjx7BI8gegx2orsERMCJ0E8Fx4gfg"
    async
  ></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js" defer="defer"></script>
      <script src="https://cdn.jsdelivr.net/npm/gis-utils@1.1.0/dist/index.min.js" defer="defer"></script>

        
  {%- if settings.enable_maintenance -%}
  {%- render 'custom-scripts' -%}
  {%- endif -%}
  {%- render 'variables' -%}
  {%- render 'css-variables' -%}
  {%- render 'font-face' -%}
  {%- render 'checkout-custom-translation'-%}
  {{ 'checkout.css' | asset_url | stylesheet_tag }}
  {{ 'xml2json.js' | asset_url | script_tag }}
   {{ 'checkout-custom.js' | asset_url | script_tag }}
    {% render 'checkout-custom' %}
    
  <script type="text/javascript">
    (function(c,l,a,r,i,t,y){
        c[a]=c[a]||function(){(c[a].q=c[a].q||[]).push(arguments)};
        t=l.createElement(r);t.async=1;t.src="https://www.clarity.ms/tag/"+i;
        y=l.getElementsByTagName(r)[0];y.parentNode.insertBefore(t,y);
    })(window, document, "clarity", "script", "edm5dfsyr0");
  </script>

  <script>
    window.maintenance_mode = {{ settings.enable_maintenance }};
    window.affiliate_id = {{ settings.affiliate_id }};
    window.supplier_fullname = {{ settings.supplier_fullname }};
  </script>

  {% render 'shogun-head' %}
</head>
  <body>
    {{ skip_to_content_link }}

    <header class="banner" data-header role="banner">
      <div class="wrap">
        {{ content_for_logo }}
      </div>
    </header>

    {{ order_summary_toggle }}
    <div class="content" data-content>
      <div class="wrap">
        <div class="main">
          <header class="main__header" role="banner">
            {{ content_for_logo }}
            {{ breadcrumb }}
            {% comment %}
            <div class="checkout__additional">
            Sign up and shop to get a chance to win ₱1,000 GrabGifts! (min. spend of ₱1,499) - <a href="/pages/penshoppe-x-grab" style="color: #da291c; text-decoration-line: underline;">Click here</a> to learn more.
            </div>
            {%- endcomment -%}
            {{ alternative_payment_methods }}
          </header>
          <main class="main__content" role="main">
            {{ content_for_layout }}
          </main>
          <footer class="main__footer" role="contentinfo">
            {{ content_for_footer }}
          </footer>
        </div>
        <aside class="sidebar" role="complementary">
          <div class="sidebar__header">
            {{ content_for_logo }}
          </div>
          <div class="sidebar__content">
            {{ content_for_order_summary }}
          </div>
        </aside>
      </div>
    </div>

    {{ tracking_code }}
 {% comment %}   {% render 'address-finder-checkout' %}
    {% render 'checkout-custom-scripts' %}
       {%- endcomment -%}
       
  </body>
</html>
