               data-src="{{ img_url }}"
               data-widths="[177, 247, 257, 268, 277, 320, 354, 400]"
               data-aspectratio="{{ block.settings.mega_menu_img_r1.aspect_ratio }}"
               data-sizes="auto"
               data-parent-fit="cover"
               alt="{{ block.settings.mega_menu_img_r1.alt | escape }}" />
  </a>
