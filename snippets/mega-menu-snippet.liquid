  {% if block.settings.megamenu_column == 'basic' %}
  <div class="mega-menu-block {{block.settings.megamenu_column}}">
  <span class="mega-menu-title">{{ block.settings.megamenu_column_title }} </span>
  {% for link in basic_menu.links %}
  <a {% if link.title contains 'SALE' %}class="sale-link"{% endif %}href="{{ link.url }}">{{ link.title }}</a>
  {% endfor %}
  </div>
  {% elsif block.settings.megamenu_column == 'image' %} 
  <a href="{{ block.settings.mega_menu_img_r1_link }}">
    {% assign img_url = block.settings.mega_menu_img_r1 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
