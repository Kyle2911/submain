{%- assign cart_weight20 = 0 -%}
{%- assign cart_weight25 = 0 -%}
{%- assign cart_weight30 = 0 -%}
{%- assign cart_weight50 = 0 -%}
{%- assign cart_weight55 = 0 -%}
{%- assign cart_weight70 = 0 -%}
{%- assign cart_weight75 = 0 -%}
{%- assign cart_weight80 = 0 -%}
{%- assign cart_weight100 = 0 -%}
{%- assign cart_weight150 = 0 -%}
{%- assign cart_weight1500 = 0 -%}
{%- assign ml_temp = 0 -%}
{%- assign frag_handles = '' -%}

{%- assign item_size = "" -%}
{%- assign ml_item_size = 0 -%}
{%- assign size_temp = 0 -%}
{%- assign cart_weight_overall = 0 -%}

{%- for item in cart.items -%}
  {%- assign ml_temp = 0 -%}
  {%- if item.product.tags contains 'fragrances' or item.product.tags contains 'sanitizer' -%}
  {%- if item.variant.title contains 'ML' -%}
  {%- assign item_size = item.variant.title | split: '/' -%}
  {%- assign ml_item_size = item_size[1] | remove: ' ML' | plus: 0 -%}
  {%- assign size_temp = ml_item_size | times: item.quantity -%}
  {%- assign cart_weight_overall = cart_weight_overall | plus: size_temp -%}
  {%- endif -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F20' -%}
  {%- assign ml_temp = 20 | times: item.quantity -%} 
    {%- assign cart_weight20 = cart_weight20 | plus: ml_temp -%}
  {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F25' -%}
  {%- assign ml_temp = 25 | times: item.quantity -%} 
    {%- assign cart_weight25 = cart_weight25 | plus: ml_temp -%}
  {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F30' -%}
  {%- assign ml_temp = 30 | times: item.quantity -%}
    {%- assign cart_weight30 = cart_weight30 | plus: ml_temp -%}
  {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F50' -%}
  {%- assign ml_temp = 50 | times: item.quantity -%}
    {%- assign cart_weight50 = cart_weight50 | plus: ml_temp -%}
  {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F55' -%}
  {%- assign ml_temp = 55 | times: item.quantity -%}
    {%- assign cart_weight55 = cart_weight55 | plus: ml_temp -%}
  {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F70' -%}
  {%- assign ml_temp = 70 | times: item.quantity -%}
    {%- assign cart_weight70 = cart_weight70 | plus: ml_temp -%}
  {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F75' -%}
  {%- assign ml_temp = 75 | times: item.quantity -%}
    {%- assign cart_weight75 = cart_weight75 | plus: ml_temp -%}
  {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F80' -%}
  {%- assign ml_temp = 80 | times: item.quantity -%}
    {%- assign cart_weight80 = cart_weight80 | plus: ml_temp -%}
  {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F100' -%}
  {%- assign ml_temp = 100 | times: item.quantity -%}
    {%- assign cart_weight100 = cart_weight100 | plus: ml_temp -%}
  {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F150' -%}
  {%- assign ml_temp = 150 | times: item.quantity -%}
    {%- assign cart_weight150 = cart_weight150 | plus: ml_temp -%}
    {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}

  {%- if item.product.tags contains 'F1500' -%}
  {%- assign ml_temp = 1500 | times: item.quantity -%}
    {%- assign cart_weight1500 = cart_weight1500 | plus: ml_temp -%}
  {%- assign frag_handles = frag_handles | append: item.product.handle | append: ',' -%}
  {%- endif -%}
{%- endfor -%}

{%- assign frag_handles = frag_handles | split: ',' -%}
{%- assign overall = cart_weight20 | plus: cart_weight25 | plus: cart_weight30 | plus: cart_weight50 | plus: cart_weight55 | plus: cart_weight70 | plus: cart_weight75 | plus: cart_weight80 | plus: cart_weight100 | plus: cart_weight150 | plus: cart_weight1500 -%}